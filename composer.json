{
    "name": "codium/laravel-sms-capture",
    "type": "library",
    "description": "Captures outgoing SMS notifications",
    "keywords": [
        "laravel",
        "notifications",
        "sms",
        "nexmo",
        "vonage"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0",
        "illuminate/notifications": "^8.0|^9.0|^10.0",
        "illuminate/broadcasting": "^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^9.3.5",
        "laravel/framework": "^8.0|^9.0|^10.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Codium\\SmsCapture\\ServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Codium\\SmsCapture\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
